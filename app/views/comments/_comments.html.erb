<% comments.includes(:parent, :user).each do |comment|%>
  <div class="collapse show" id="collapseComment<% comment.id%>">
    <%= turbo_frame_tag comment do %>
      <% unless comment.parent %>
        <%= render 'comments/comment', comment: comment, post: post %>  
      <% end %>    
    <% end %>
  </div>    
<% end %>


