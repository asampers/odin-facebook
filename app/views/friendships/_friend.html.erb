<div id="<%= dom_id user %>">
  <li class="d-flex align-items-center justify-content-start list-group-item mb-1 rounded user"> 
    <%= image_tag user.gravatar_url(default: :identicon), class:"rounded-circle img-fluid gravatar-home me-2" %>
    <%= link_to "#{user.username}", user_path(user.id), class: "align-items-center" %> 
  </li>
</div>