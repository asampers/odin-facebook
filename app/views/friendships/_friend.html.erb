<div id="<%= dom_id user %>">
  <ul class="list-group">
    <li class="d-flex align-items-center justify-content-start list-group-item"> 
      <%= image_tag user.gravatar_url(default: :mm), class:"rounded-circle img-fluid gravatar-home me-2" %>
      <%= link_to "#{user.username}", user_path(user.id), class: "align-items-center" %> </li>
  </ul>
</div>